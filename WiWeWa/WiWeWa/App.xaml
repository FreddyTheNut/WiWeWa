             xmlns:vm="clr-namespace:SMPHCracker.ViewModel"
             x:Class="WiWeWa.App">
	<Application.Resources>

        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="ViewModelLocator"/>

            <Color x:Key="BackgroundColor">#000000</Color>

            <Color x:Key="PrimaryColor">#000b49</Color>
            <Color x:Key="PrimaryLightColor">#373075</Color>
            <Color x:Key="PrimaryDarkColor">#000023</Color>

            <Color x:Key="SecondaryColor">#005142</Color>
            <Color x:Key="SecondaryLightColor">#3a7e6d</Color>
            <Color x:Key="SecondaryDarkColor">#00281b</Color>
        </ResourceDictionary>

    </Application.Resources>
